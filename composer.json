{
    "name": "thomasnordahldk/tester",
    "description": "An object oriented approach to testing for PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Pedersen",
            "email": "thno@fynskemedier.dk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "phpunit/php-code-coverage": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "ThomasNordahlDk\\Tester\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThomasNordahlDk\\Tester\\Tests\\": "tests/"
        }
    },
    "bin": ["dist/tester"]
}
